<Window x:Class="DefaultPlugin.OtherDialog.MergeFileOutputDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DefaultPlugin.ConfigDialog"
        mc:Ignorable="d"
        Title="Save File to" Height="80" Width="400"
        WindowStartupLocation="CenterScreen" ResizeMode="CanMinimize">
    <Grid Background="Black">
        <StackPanel Orientation="Vertical">
            <StackPanel Orientation="Horizontal" Height="30" Margin="0,10">
                <TextBox x:Name="OutputFileTextBox" Width="280" ></TextBox>
                <Button x:Name="BrowseButton" Width="120" Click="BrowseButton_Click">Browse</Button>
            </StackPanel>
        </StackPanel>
    </Grid>
</Window>
